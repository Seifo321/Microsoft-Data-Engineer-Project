{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-depi-proj"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"onpremisesqlserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'onpremisesqlserver'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakemfyghw5.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakemfyghw5.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://seifo-keyvault.vault.azure.net/"
		},
		"onpremisesqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "XD\\MSSQLSERVER2024"
		},
		"onpremisesqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "BikeStores"
		},
		"onpremisesqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "XD\\lenovo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-depi-proj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"onpremisesqlserver_password": {
						"value": "[parameters('onpremisesqlserver_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"onpremisesqlserver_properties_typeProperties_server": {
						"value": "[parameters('onpremisesqlserver_properties_typeProperties_server')]"
					},
					"onpremisesqlserver_properties_typeProperties_database": {
						"value": "[parameters('onpremisesqlserver_properties_typeProperties_database')]"
					},
					"onpremisesqlserver_properties_typeProperties_userName": {
						"value": "[parameters('onpremisesqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}